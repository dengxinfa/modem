#telecom
config interface 'modem'
        option country 'country13'
        option sp 'isp0'
        option dialnum '#777'
        option apn 'ctnet'
        option username 'ctnet@mycdma.cn'
        option password 'vnet.mobi'
        option metric '40'
        option proto '3g'
        option ifname '3g-modem'
        option device '/dev/ttyUSB0'
        option service 'evdo'
